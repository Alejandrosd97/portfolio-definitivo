<style>
	
</style>

    <div class="cursor" id="cursor"></div>

<script>
	let cursor = document.getElementById('cursor')
	document.addEventListener('mousemove', (e)=>{
		if (cursor){
			cursor.style.left = e.clientX + 'px'
			cursor.style.top = e.clientY + 'px'
		}
	})
</script>